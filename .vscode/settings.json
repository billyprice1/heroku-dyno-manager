// Place your settings in this file to overwrite default and user settings.
{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/node_modules": true,
		"**/.idea": true,
		"**/.tmp": true,
		"**/.sass-cache": true,
		"**/.DS_Store": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/node_modules/**": true,
		"**/.idea/**": true,
		"**/.tmp/**": true,
		"**/e2e/**": true,
		"**/dist/**": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/.idea": true,
		"**/.tmp": true,
		"**/.sass-cache": true,
		"**/dist": true
	}
}